         <script>
          let cod = document.getElementById("cod").innerHTML
          let PayPal = document.getElementById("paypal").innerHTML
          let RazorPay = document.getElementById("razorpay").innerHTML
          let noofcod = document.getElementById("numberofcod").innerHTML
          let noofpaypal = document.getElementById("numberofpaypal").innerHTML
          let noofrazorpay = document.getElementById("numberofrazorpay").innerHTML
          let noofcancelled = document.getElementById("numberofcancelled").innerHTML
          console.log(noofcod,noofpaypal,noofrazorpay,noofcancelled);
          console.log(cod+"COD",PayPal+"PAYAP",RazorPay+"RAX");
          const ctx = document.getElementById('myChart').getContext('2d');
          const myChart = new Chart(ctx, {
              type: 'line',
              data: {
                  labels: ['Cash On Delivery ', 'PayPal', 'RazorPay'],
                  datasets: [{
                      label: 'Revenue',
                      data: [cod, PayPal, RazorPay],
                      backgroundColor: [
                          'rgba(255, 99, 132, 0.2)',
                          'rgba(54, 162, 235, 0.2)',
                          'rgba(255, 206, 86, 0.2)',
                          'rgba(75, 192, 192, 0.2)',
                          'rgba(153, 102, 255, 0.2)',
                          'rgba(255, 159, 64, 0.2)'
                      ],
                      borderColor: [
                          'rgba(255, 99, 132, 1)',
                          'rgba(54, 162, 235, 1)',
                          'rgba(255, 206, 86, 1)',
                          'rgba(75, 192, 192, 1)',
                          'rgba(153, 102, 255, 1)',
                          'rgba(255, 159, 64, 1)'
                      ],
                      borderWidth: 1
                  }]
