                  <table id="admin-table" class="table">
                    <thead>
                      <tr>
                        <th>User</th>
                        <th>OrderId</th>
                        <th>Date</th>
                        <th>Total Price</th>
                        <th>Status</th>
                      </tr>
                    </thead>
                    <% for (var i = 0; i < numberoforder.length;  i++ ) { %>
                      <% if (numberoforder[i].date==todaydate) { %>
                    <tbody>
                      <tr>
                        <td><%=numberoforder[i].username%></td>
                        <td><%=numberoforder[i]._id%></td>
                        <td><%=numberoforder[i].date%></td>
                        <td>₹ <%=numberoforder[i].totalprice%></td>
                       
                          <td><% if (numberoforder[i].status==="placed") { %>
                            <label class="badge badge-success">Placed</label>
                            <%}%> 
                            <% if (numberoforder[i].status==="Delivered") { %>
                              <label class="badge badge-success">Delivered</label>
                              <%}%>
                            <% if (numberoforder[i].status==="Cancelled") { %>
                              <label class="badge badge-danger">Cancelled</label>
                            <% } %>
                            <% if (numberoforder[i].status==="Pending") { %>
                              <label class="badge badge-danger">Pending</label>
                            <% } %>
                            <% if (numberoforder[i].status==="Failed") { %>
                              <label class="badge badge-danger">Failed</label>
                            <% } %>
                            <% if (numberoforder[i].status==="Returning") { %>
                              <label class="badge badge-danger">Returning</label>
                            <% } %>
                            <% if (numberoforder[i].status==="Shipped") { %>
                              <label class="badge badge-success">Shipped</label>
                            <% } %>
                            </td>
                      </tr>
                    </tbody>
                    <% } %>
                    <% } %>                      
                  </table>
                </div>
              </div>
            </div>
