                                <% if (error.length > 2) { %>
                                    <h1 class="page-header text-center">Admin Login</h1>
                                    <br>
                                    <h4 style="color: red;">!!<%=error%>!!</h4>
                                    <%} else { %>
                                        <h1 class="page-header text-center">Admin Login</h1>
                                    <% } %>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6 ">
                        <div class="row">
                            <div class="col-md-12">
                                <form method="post" action="/admin/login" id="f1" name="f1" accept-charset="UTF-8"><input type="hidden"  value="customer_login" /><input type="hidden" name="utf8" value="âœ“" />
                                
                                <label>Email</label>
                                <input type="email"  value="" name="email" class="form-control margin-bottom-20" id="email" />
                                <p id="email1"></p>
                                <label>Password</label>
                                <input type="password" value="" name="password" class="form-control margin-bottom-20" id="password" size="16" />
                                <p id="password1"></p>
                                <button type="submit" onclick="return validate()" class="btn btn-default pull-left margin-top-10 btn-lg">Log In</button>
                               
                                
                                </form>
                                <script>
                                    
             function validate() {
        
              
        
              
           
              if(document.f1.email.value ==="" )
              {
                
               document.getElementById('email').style.borderColor="red";
               document.f1.email.focus() 
               document.getElementById("email1").innerHTML="*Fill this filed"
               document.getElementById("email1").style.color="red";
               return false;
             }
             else if( !isNaN(document.f1.email.value) ) 
          {
        
                document.getElementById('email').style.borderColor="red";
                document.f1.email.focus() 
                document.getElementById("email1").innerHTML="*Enter a Valid Email";
                document.getElementById("email1").style.color="red";
                    return false; 
                }
                    
            
             
             else if( document.f1.password.value ==="" )
              {
                
                document.getElementById('password').style.borderColor="red";
               document.f1.password.focus() 
               document.getElementById("password1").innerHTML="*Fill this filed"
               document.getElementById("password1").style.color="red";
               return false;
             }
             else{
        
             }
            }
        
                            </script>
                            </div>
                        </div>
