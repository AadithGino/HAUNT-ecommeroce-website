                                <input type="email" placeholder="Email Address" value="" size="30" name="email" id="recover-email" class="form-control margin-bottom-20" />
                                <button type="submit" class="btn btn-default pull-left margin-top-10">Reset</button>
                                <button onclick="hideRecoverPasswordForm();return false;" class="btn btn-default pull-left margin-left-10 margin-top-10">Cancel</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
