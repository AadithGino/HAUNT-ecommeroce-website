      <div class="main-panel">          
        <div class="content-wrapper">
          <div class="row">
              <div class="col-lg-12">
                  <div class="card px-2">
                      <div id="invoice" class="card-body">
                        <% for (var i =0; i < order.length;  i++ ) { %>
                          <div class="container-fluid">
                            <h3 class="text-right my-5">Invoice&nbsp;&nbsp;#<%=order[i]._id%></h3>
                            <hr>
                          </div> 
                          

                          <div class="container-fluid d-flex justify-content-between">
                            <div class="col-lg-3 ps-0">
                              <p class="mt-5 mb-2"><b>Haunt</b></p>
                              <p>Kochi,<br>Eranakulam,<br>Kerala.</p>
                            </div>
                            <div class="col-lg-3 pr-0">
                              <p class="mt-5 mb-2 text-right"><b>Invoice to</b></p>
                              <p class="text-right"><%=order[i].address[0].city%>,<br> <%=order[i].address[0].address%>,<br> <%=order[i].address[0].zip%>,<br><%=order[i].address[0].country%></p>
                            </div>
                          </div>
                          <div class="container-fluid d-flex justify-content-between">
                            <div class="col-lg-3 ps-0">
                              <% if (order[i].status=="Cancelled") { %>
                                  <h4 style="color: red;" class="text-left ">Order Status : Cancelled</h4>
                                  <% } %>
                                  <% if (order[i].status=="Failed") { %>
                                    <h4 style="color: red;" class="text-left ">Order Status : Failed</h4>
                                    <% } %>
                                    <% if (order[i].status=="Returning") { %>
                                      <h4 style="color: red;" class="text-left ">Order Status : Returning</h4>
                                      <% } %>
                                    <% if (order[i].status=="Pending") { %>
                                      <h4 style="color: red;" class="text-left ">Order Status : Pending</h4>
                                      <% } %>
                                      <% if (order[i].status=="placed") { %>
                                        <h4 style="color: green;" class="text-left ">Order Status : Placed</h4>
                                        <% } %>
                                        <% if (order[i].status=="Shipped") { %>
                                          <h4 style="color: green;" class="text-left ">Order Status : Shipped</h4>
                                          <% } %>
                                          <% if (order[i].status=="Delivered") { %>
                                            <h4 style="color: green;" class="text-left ">Order Status : Delivered</h4>
                                            <% } %>
                                        
                            <p class="mt-5 ">Name:  <%=order[i].username%> </p>
                            <p class="mb-0 mt-5">Invoice Date : <%=order[i].date%></p>
                            <p class="mb-0 mt-5">Payment Status  : <%=order[i].payment%></p>
                            
                          </div>
                          </div>
                          <div class="container-fluid mt-5 d-flex justify-content-center w-100">
                            <div class="table-responsive w-100">
                                <table class="table">
                                  <thead>
                                    <tr class="bg-dark text-white">
                                        <th>#</th>
                                        <th>Product Name</th>
                                        <th class="text-right">Quantity</th>
                                        <th class="text-right">Unit cost</th>
                                      </tr>
                                  </thead>
                                  <% for (var j = 0; j < order[i].products.length;  j++ ) { %>
                                    
                                    
                                    
                                  <tbody>
                                    <tr class="text-right">
                                      <td class="text-left"><%=j+1%></td>
                                      <td class="text-left"><%=order[i].products[j].product_name%></td>
                                      <td><%=order[i].products[j].product_quantity%></td>
                                      <td><%=order[i].products[j].product_price%></td>
                                    </tr>
                                  </tbody>
                                  <% } %>
                                </table>
                              </div>
                          </div>
                          
                            <div class="container-fluid mt-5 w-100">
                              <% if (order[i].coupon != "No Coupon Used") { %>
                                <h4 class="text-right mb-5">Counpon : ₹ <%=order[i].coupon%></h4>
                                <% } %>
                              <h4 class="text-right mb-5">Total : ₹ <%=order[i].totalprice%></h4>
                              <% if (order[i].status=="Cancelled") { %>
                                  <h4 style="color: red;" class="text-right mb-5">Cancelled</h4>
                                  <% } %>
                                  <% if (order[i].status=="Failed") { %>
                                    <h4 style="color: red;" class="text-right mb-5">Order Status : Failed</h4>
                                    <% } %>
                                    <% if (order[i].status=="Pending") { %>
                                      <h4 style="color: red;" class="text-right mb-5">Order Status : Pending</h4>
                                      <% } %>
                                      <% if (order[i].status=="Returning") { %>
                                        <h4 style="color: red;" class="text-right mb-5">Order Status : Returning</h4>
                                        <% } %>
                                      <% if (order[i].status=="placed") { %>
                                        <h4 style="color: green;" class="text-right mb-5">Order Status : Placed</h4>
                                        <% } %>
                                        <% if (order[i].status=="Shipped") { %>
                                          <h4 style="color: green;" class="text-right mb-5">Order Status : Shipped</h4>
                                          <% } %>
                                          <% if (order[i].status=="Delivered") { %>
                                            <h4 style="color: green;" class="text-right mb-5">Order Status : Delivered</h4>
                                            <% } %>
                                        
                                   
                              <hr>
                            </div>
                          </div>
                          
                      </div>
                      <% } %>
                      <div class="container-fluid w-100">
                        <a id="download" class="btn btn-success float-right mt-4"><i class="fa-solid fa-download"></i>&nbsp; Download Invoice</a>
