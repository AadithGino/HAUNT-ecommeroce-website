          <div class="responsive-table">
           
            <table class="table table-bordered table-fluid" id="myTable">
              <thead>
                  <tr>
                      
                      <th>Username</th>
                      <th>OrderId</th>
                      <th>Total Amount</th>
                      <th>Order Date</th>
                      <th>Order Status</th>
                  </tr>
              </thead>
              <tbody>
                <% for(var i=0;i<=monthlyorder.length-1; i++){ %>
                  <tr>
                     
                      <td>
                        <%=monthlyorder[i].username%>
                      </td>
                      <td>
                        <%=monthlyorder[i]._id%>
                      </td>
                      <td>
                        ₹ <%=monthlyorder[i].totalprice%>
                      </td>
                      <td>
                         <%=monthlyorder[i].date%>
                      </td>
                      <td>
                        <% if (monthlyorder[i].status==="placed") { %>
                          <label class="badge badge-success">Placed</label>
                          <%}%> 
                          <% if (monthlyorder[i].status==="Cancelled") { %>
                            <label class="badge badge-danger">Cancelled</label>
                          <% } %>
                          <% if (monthlyorder[i].status==="Pending") { %>
                            <label class="badge badge-danger">Pending</label>
                          <% } %>
                          <% if (monthlyorder[i].status==="Failed") { %>
                            <label class="badge badge-danger">Failed</label>
                          <% } %>
                          <% if (monthlyorder[i].status==="Returning") { %>
                            <label class="badge badge-danger">Returning</label>
                          <% } %>
                          <% if (monthlyorder[i].status==="Delivered") { %>
                            <label class="badge badge-success">Delivered</label>
                            <%}%> 
                            <% if (monthlyorder[i].status==="Shipped") { %>
                              <label class="badge badge-primary">Shipped</label>
                              <%}%> 
                      </td>
                    
                      
                      
                  </tr>
                  <%}%>
              </tbody>
          </table>
