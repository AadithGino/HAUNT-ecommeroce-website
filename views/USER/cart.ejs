<main class="cart-page margin-bottom-30">
    <section class="cart-content">
        <div class="container text-center">
            <div class="row">
                <div class="col-md-12">
                    <div class="page-title margin-top-30">
                        <h1 class="page-header text-center">Shopping Cart</h1>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12 margin-top-30 text-center paira-cartEmpty" style="display: none;">
                    <h3 class="text-capitalize margin-top-40">It Appears That Your Cart Is Currently Empty!</h3>
                    <h5 class="margin-bottom-10">Please Continue Shopping ...</h5>
                </div>
                <!-- <form action method="post" class="paira-cartNotEmpty" > -->
                    <div class="col-md-8 col-sm-7 col-xs-12 margin-top-40">
                        <div class="cart-item-list">
                            <h6 class="text-uppercase panel-title cart-title">Product Lists</h6>
                            <ul class="list-unstyled paira-cart-page-list">
                                
                                <% if (cart.length < 1) { %>
                                    <h3 class="text-capitalize margin-top-40">It Appears That Your Cart Is Currently Empty!</h3>
                    <h5 class="margin-bottom-10">Please Continue Shopping ...</h5>
                                    <%} else { %>
                                    
                                        <% for (var i =0; i < cart.length;  i++ ) { %>

                                            <li>
                                                <div class="col-md-2 col-sm-3 col-xs-12 margin-top-20">
                                                    <a href="/singleproduct?id=<%= cart[i].products[0]['_id'] %>"">
                                                        <img class="img-responsive thumbnail" src="<%=cart[i].products[0].image1%>" alt="Free demo product name 5 - m / blue / cotton">
                                                    </a>
                                                </div>
                                               
                                                <div class="col-md-10 col-sm-9 col-xs-12 margin-top-20">
                                                    <a href="/products/free-demo-product-name-5?variant=28315184721" class="margin-top-10"><strong>Product : <%=cart[i].products[0].itemname%></strong></a>
                                                    <h4 class="margin-top-20 margin-bottom-10"><span class="money">Price : <span ><del><%=cart[i].products[0].price%></del></span>  <% if (cart[i].products[0]['originalprice'] > cart[i].products[0]['categoryprice']) { %>   ₹ <%= cart[i].products[0]['categoryprice'] %>   <%} else { %>   ₹ <%= cart[i].products[0]['originalprice'] %>   <% } %></span></h4>
                                                    <h4 class="margin-top-20 margin-bottom-10"><span class="money">Total Price : ₹ <span id="<%= cart[i].products[0]['_id'] %>"><% if (cart[i].products[0]['originalprice'] > cart[i].products[0]['categoryprice']) { %>    <%= cart[i].products[0]['categoryprice'] * cart[i].quantity  %>   <%} else { %>    <%= cart[i].products[0]['originalprice'] * cart[i].quantity %>   <% } %></span> </span></h4>
                                                      
                                                    
                                                    <div class="full-width cart-update-fix">
                                                        <% if (cart[i].products[0].quantity < 5) { %>
                                                            <label style="color: red;"  class="margin-top-10">Stock Left : <%=cart[i].products[0].quantity%></label>
                                                            <% } %>
                                                            <% if (cart[i].quantity > cart[i].products[0].quantity) { %>
                                                                <label style="color: red;"  class="margin-top-10"> Only <%=cart[i].products[0].quantity%> stock left reduce the quantity to Continue </label>
                                                                <% } %>
                                                            
                                                       

                                                    <div class="full-width cart-update-fix">
                                                        <div class="quantity-field" >

                                                            <% if (cart[i].products[0]['originalprice'] > cart[i].products[0]['categoryprice']) { %>
                                                                <button 
                                                              class="value-button decrease-button" 
                                                              onclick="decreaseValue(this, `<%=cart[i].product._id%>`,`<%=cart[i].products[0].categoryprice%>`)" 
                                                              title="Azalt">-</button>
                                                              <div id="qustock" class="number"><%=cart[i].quantity%></div>
                                                             
                                                            <button 
                                                              class="value-button increase-button" 
                                                              onclick="increaseValue(this, `<%=cart[i].product._id%>`,`<%=cart[i].products[0].quantity%>`,`<%=cart[i].products[0].categoryprice%>`)"
                                                              title="Arrtır"
                                                            >+
                                                            </button>
                                                                <%} else { %>
                                                                    <button 
                                                                    class="value-button decrease-button" 
                                                                    onclick="decreaseValue(this, `<%=cart[i].product._id%>`,`<%=cart[i].products[0].originalprice%>`)" 
                                                                    title="Azalt">-</button>
                                                                    <div class="number"><%=cart[i].quantity%></div>
                                                                  <button 
                                                                    class="value-button increase-button" 
                                                                    onclick="increaseValue(this, `<%=cart[i].product._id%>`,`<%=cart[i].products[0].quantity%>`,`<%=cart[i].products[0].originalprice%>`)"
                                                                    title="Arrtır"
                                                                  >+
                                                                  </button>

                                                                <% } %>
                                                                
                                                            
                                                          </div>
                                                        <a onclick="cartremove(`<%=cart[i].product._id%>`)"   class=" btn-default pull-right paira-cart-page-delete  margin-bottom-10 btn-lg"><i class="fa fa-times"></i> Remove</a>
                                                    </div>
                                                </div>
                                            </li>
                                            
                                            
                                   <% } %>
                            </ul>
                        </div>
                    </div>
                    <aside class="col-md-4 col-sm-5 col-xs-12 margin-top-40">
                        <figure class="margin-bottom-30">
                            <figcaption><h4 class="panel-heading">Sub Total</h4></figcaption>
                            <div class="cart-sub-total paira-cart-sub-total">
                                
                                    
                                <h2 class="margin-top-5 margin-bottom-20">Rs : &nbsp;<span id="totalprice" class="money"><%=total[0].total%></span></h2>
                                <textarea id="note" name="note" class="margin-top-10 margin-bottom-10 form-control" placeholder="Special instructions..."></textarea>
                              
                                    <% let allItemInStock = true;  for (var i =0; i < cart.length;  i++ ) { 
                                        if (cart[i].quantity > cart[i].products[0].quantity) { allItemInStock = false } }
                                       if(allItemInStock) { %>
                                        <a href="/selectaddress"><input type="submit" id="checkout" class="btn btn-default btn-lg btn-block text-uppercase" name="checkout" value="Proceed To Checkout"></a>
                                       <% } else { %>
                                        <a><input style="color: red;" type="submit" id="checkout1" class="btn btn-default btn-lg btn-block text-uppercase" name="checkout" value="Cannot Checkout"></a>
                                       <% } %>
                                       <a style="display: none;" href="/selectaddress"><input type="submit" id="checkout2" class="btn btn-default btn-lg btn-block text-uppercase" name="checkout" value="Proceed To Checkout"></a>
                                
                            </div>
                        </figure>
                        <script>
                        

                        
                        </script>
                    </aside>
                    
                    <% } %>
                </form>
            </div>
        </div>
    </section>
</main>

