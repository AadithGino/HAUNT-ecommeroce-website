                        <div class="text-center">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="page-title margin-bottom-30 margin-top-30">
                                        <% if (error.length > 2) { %>
                                            <h1 class="page-header text-center">Login</h1>
                                            <br>
                                            <h4 style="color: red;">!!<%=error%>!!</h4>
                                            <%} else { %>
