                        </div>
                        <div class="row">
                            <div class="col-md-6 ">
                                <div class="row">
                                    <div class="col-md-12">
                                        <form method="post" action="/otpverify" id="f1" name="f1"
                                            accept-charset="UTF-8"><input type="hidden" value="customer_login" /><input
                                                type="hidden" name="utf8" value="✓" />

                                            <label>OTP</label>
                                            <input type="number" placeholder="example:123456" value="" name="email"
                                                class="form-control margin-bottom-20" id="email" />
                                            <p id="email1"></p>
                                            <button type="submit" onclick="return validate()"
                                                class="btn btn-default pull-left margin-top-10 btn-lg">Log In</button>
                                            <!-- <a href="/otplogin" class="pull-left margin-top-10"><h5> &nbsp; &nbsp;&nbsp; &nbsp;Resend OTP?</h5></a>  -->
                                            <a href="/userlogin" class="pull-right margin-top-10">
                                                <h5>Login with Password</h5>
                                            </a>

                                        </form>
                                        <script>

                                            function validate() {





                                                if (document.f1.email.value === "") {

                                                    document.getElementById('email').style.borderColor = "red";
                                                    document.f1.email.focus()
                                                    document.getElementById("email1").innerHTML = "*Fill this filed"
                                                    document.getElementById("email1").style.color = "red";
                                                    return false;
                                                }
                                                else if (document.f1.email.value.length < 6 && document.f1.email.value.length > 6) {
                                                    console.log(document.f1.email.value.length);
                                                    document.getElementById('email').style.borderColor = "red";
                                                    document.f1.email.focus()
                                                    document.getElementById("email1").innerHTML = "*Enter a Valid OTP";
                                                    document.getElementById("email1").style.color = "red";
                                                    return false;
                                                }



                                                else if (document.f1.password.value === "") {

                                                    document.getElementById('password').style.borderColor = "red";
                                                    document.f1.password.focus()
                                                    document.getElementById("password1").innerHTML = "*Fill this filed"
                                                    document.getElementById("password1").style.color = "red";
                                                    return false;
                                                }
                                                else {

                                                }
                                            }

                                        </script>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 text-center">
                                <h4 class="margin-bottom-20 margin-top-40">New Customer?</h4>
                                <a href="/signup" class="btn btn-primary btn-lg">Create An Account</a>
                            </div>
